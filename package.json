  "scripts": {
    "build": "tsc",
    "start": "nodemon",
    "commit": "git add --all && npx git cz",
    "migrations_run": "docker exec -it server npx ts-node ./node_modules/typeorm/cli migration:run",
    "migrations_gen": "docker exec -it server npx ts-node ./node_modules/typeorm/cli migration:generate -n ${name}",
    "migrations_create": "docker exec -it server npx ts-node ./node_modules/typeorm/cli migration:create -n ${name}",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "config": {
    "perfix_prod": "production",
    "perfix_staging": "release",
    "commitizen": {
      "path": "node_modules/cz-customizable"
